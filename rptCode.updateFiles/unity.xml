<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="unityConsole" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
    <section name="unityCommon" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>
    <section name="unityHms" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>   
	<section name="unityRptFunc" type = "Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration"/>   	
  </configSections>
  <unityConsole>
    <namespace name="Console.Itf" />
    <assembly name="Console.Itf, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    <containers>
      <container name="login">
        <register type="Console.Itf.ItfLogin,Console.Itf" mapTo="Console.Svc.SvcLogin,Console.Biz"/>
      </container>
      <container name="frame">
        <register type="Console.Itf.ItfFrame,Console.Itf" mapTo="Console.Svc.SvcFrame,Console.Biz"/>
      </container>
      <container name="dictionary">
        <register type="Console.Itf.ItfDictionary,Console.Itf" mapTo="Console.Svc.SvcDictionary,Console.Biz"/>
      </container>
    </containers>
  </unityConsole> 
  <unityCommon>
    <namespace name="Common.Itf" />
    <assembly name="Comm.Itf, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    <containers>
      <container name="common">
        <register type="Common.Itf.ItfCommon,Comm.Itf" mapTo="Common.Svc.SvcCommon,Comm.Biz"/>
      </container>
      <container name="entityfactory">
        <register type="Common.Itf.ItfEntityFactory,Comm.Itf" mapTo="Common.Svc.SvcEntityFactory,Comm.Biz"/>
      </container>
      <container name="formdesign">
        <register type="Common.Itf.ItfFormDesign,Comm.Itf" mapTo="Common.Svc.SvcFormDesign,Comm.Biz"/>
      </container>
    </containers>
  </unityCommon> 
  <unityHms>
    <namespace name="Hms.Itf" />
    <assembly name="hms.itf, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    <containers>
      <container name="hms">
        <register type="Hms.Itf.ItfHms,Hms.Itf" mapTo="Hms.Svc.SvcHms,Hms.Biz"/>
      </container> 
    </containers>
  </unityHms> 
  <unityRptFunc>
    <namespace name="RptFunc.Itf" />
    <assembly name="rptFunc.itf, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
    <containers>
      <container name="rptFunc">
        <register type="RtpFunc.Itf.ItfRptFunc,RptFunc.Itf" mapTo="RtpFunc.Svc.SvcRptFunc,RptFunc.Biz"/>
      </container> 
    </containers>
  </unityRptFunc> 
  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
</configuration>
